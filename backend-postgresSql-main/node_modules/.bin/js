#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/React/projekan/backend-postgresSql-main/node_modules/.pnpm/js@0.1.0/node_modules/js/bin/node_modules:/mnt/d/React/projekan/backend-postgresSql-main/node_modules/.pnpm/js@0.1.0/node_modules/js/node_modules:/mnt/d/React/projekan/backend-postgresSql-main/node_modules/.pnpm/js@0.1.0/node_modules:/mnt/d/React/projekan/backend-postgresSql-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/React/projekan/backend-postgresSql-main/node_modules/.pnpm/js@0.1.0/node_modules/js/bin/node_modules:/mnt/d/React/projekan/backend-postgresSql-main/node_modules/.pnpm/js@0.1.0/node_modules/js/node_modules:/mnt/d/React/projekan/backend-postgresSql-main/node_modules/.pnpm/js@0.1.0/node_modules:/mnt/d/React/projekan/backend-postgresSql-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js/bin/js" "$@"
else
  exec node  "$basedir/../js/bin/js" "$@"
fi
